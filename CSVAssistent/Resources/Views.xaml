<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="clr-namespace:CSVAssistent.ViewModel"
    xmlns:views="clr-namespace:CSVAssistent.View.Pages">

    <DataTemplate DataType="{x:Type vm:AppSettingsViewModel}">
        <views:AppSettings />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ErrorListViewModel}">
        <views:ErrorList />
    </DataTemplate>

</ResourceDictionary>
